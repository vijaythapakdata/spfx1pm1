{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "first-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/firstWebPart/FirstWebPartWebPart.js",
          "manifest": "./src/webparts/firstWebPart/FirstWebPartWebPart.manifest.json"
        }
      ]
    },
    "sample-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleForm/SampleFormWebPart.js",
          "manifest": "./src/webparts/sampleForm/SampleFormWebPart.manifest.json"
        }
      ]
    },
    "functional-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/functionalForm/FunctionalFormWebPart.js",
          "manifest": "./src/webparts/functionalForm/FunctionalFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FirstWebPartWebPartStrings": "lib/webparts/firstWebPart/loc/{locale}.js",
    "SampleFormWebPartStrings": "lib/webparts/sampleForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "FunctionalFormWebPartStrings": "lib/webparts/functionalForm/loc/{locale}.js"
  }
}
